{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test.ts",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/tests/index.ts",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/tsx",
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000/",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
