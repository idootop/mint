{
  "name": "mint",
  "description": "薄荷的花语是：永不消逝的爱",
  "version": "3.4.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "preview": "serve out",
    "update-deps": "pnpm update -i --latest",
    "clear-image-cache": "tsx scripts/clear-image-cache.ts",
    "remove-unused-image": "tsx scripts/remove-unused-image.ts",
    "build": "pnpm clear-image-cache && next build && pnpm remove-unused-image"
  },
  "dependencies": {
    "next": "^14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "xsta": "^2.0.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.17.8",
    "@types/react": "^18.2.10",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "got": "^14.2.1",
    "https-proxy-agent": "^7.0.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "next-file-loader": "^1.0.0",
    "prettier": "^3.2.5",
    "proper-lockfile": "^4.1.2",
    "rehype-prism-plus": "^2.0.0",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.3",
    "tsx": "^4.7.2",
    "typescript": "^5.1.3"
  }
